{
    "name": "order_item_connector",
    "config": {
        "connector.class": "org.apache.iceberg.connect.IcebergSinkConnector",
        "topics": "my_order_items",

        "key.converter": "org.apache.kafka.connect.storage.StringConverter",
        "value.converter": "io.confluent.connect.avro.AvroConverter",
        "value.converter.schema.registry.url": "http://schema-registry:8081",

        "iceberg.catalog": "ice",
        "iceberg.catalog.type": "rest",
        "iceberg.catalog.uri": "http://iceberg-rest:8181",
        "iceberg.catalog.warehouse": "s3a://warehouse/",
        "iceberg.tables": "demo.order_items",
        "iceberg.tables.auto-create-enabled": "false",
        "iceberg.control.commitIntervalMs": "1000",
        "iceberg.io-impl": "org.apache.iceberg.aws.s3.S3FileIO",
        "iceberg.s3.endpoint": "http://minio:9000",
        "iceberg.s3.path-style-access": "true",
        "iceberg.s3.region": "us-east-1",
        "iceberg.catalog.client.region": "us-east-1",
        "iceberg.s3.access-key-id": "minioadmin",
        "iceberg.s3.secret-access-key": "minioadmin"
    }
}
